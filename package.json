{
  "name": "ngx-typeahead",
  "version": "6.3.0",
  "description": "A simple but yet powerful typeahead json/http component for Angular",
  "main": "./bundles/ngx-typeahead.umd.js",
  "module": "./esm5/ngx-typeahead.js",
  "es2015": "./esm2015/ngx-typeahead.js",
  "scripts": {
    "build": "node build.js",
    "test": "karma start",
    "test:watch": "karma start --single-run=false",
    "pack:lib": "npm run build && npm pack ./dist",
    "publish:lib": "npm publish ./dist",
    "publish:lib:next": "npm run build && npm publish --tag next ./dist",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc-serve": "compodoc -s"
  },
  "typings": "./ngx-typeahead.d.ts",
  "author": "Oren Farhi (orizens.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/robpoyntz/ngx-typeahead.git"
  },
  "bugs": {
    "url": "https://github.com/robpoyntz/ngx-typeahead/issues"
  },
  "homepage": "https://github.com/robpoyntz/ngx-typeahead",
  "keywords": [
    "angular",
    "javascript",
    "typescript",
    "typeahead",
    "jsonp",
    "ui component"
  ],
  "license": "MIT",
